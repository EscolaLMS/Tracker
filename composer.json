{
  "name": "escolalms/tracker",
  "description": "Escola Headless LMS Tracker",
  "type": "package",
  "require": {
    "php": "^7.4|^8.0",
    "laravel/framework": "^8.0",
    "escolalms/core": "^1.2.2",
    "escolalms/auth": "^0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tomasz Smolarek",
      "email": "tomasz.smolarek@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Tracker\\": "src",
      "EscolaLms\\Tracker\\Tests\\": "tests",
      "EscolaLms\\Tracker\\Database\\Factories\\": "database/factories",
      "EscolaLms\\Tracker\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Tracker\\EscolaLmsTrackerServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
